{
  "name": "sheets-javascript-webpack",
  "version": "3.0.0",
  "description": "How to read google sheets with google API on Heroku",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "cd client && webpack --mode=production --config webpack.config.js",
    "start": "npm run build && node server/index.js",
    "deploy": "git add . && git commit -m Heroku && git push && git push heroku && heroku logs --tail --remote heroku",
    "oldstart": "node server",
    "oldbuild": "cd react-ui/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "dotenv": "^16.0.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "css-loader": "^6.7.1",
    "express": "^4.17.3",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1"
  },
  "devDependencies": {
    "kill-port": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "sheets",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT"
}
