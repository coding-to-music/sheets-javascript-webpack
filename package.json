{
  "name": "sheets-javascript-quickstart",
  "version": "3.0.0",
  "description": "How to read google sheets with google API on Heroku",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "npm run build && node server/index.js",
    "deploy": "git add . && git commit -m Heroku && git push && git push heroku && heroku logs --tail --remote heroku",
    "oldstart": "node server",
    "oldbuild": "cd react-ui/ && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.16.4",
    "kill-port": "^1.6.1"
  },
  "devDependencies": {
    "css-loader": "^3.2.1",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "node",
    "sheets",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT"
}
